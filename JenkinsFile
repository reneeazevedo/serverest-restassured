pipeline {
    agent any
    stages {
        stage('Test') {
        steps {
            script{

                try{
                   bat 'mvn clean test'
                }catch(Exception e) {
                    print("${e}")
                }


                }
            }
        }
        stage('Allure Report') {
            steps {
                allure results: [[path: 'allure-results']]
        }
    }
  }
}